{
  "manifest_version": 2,
  "name": "Note Making Extension",
  "version": "0.1",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["libraries/p5.js", "libraries/p5.dom.js", "libraries/p5.sound.js", "sketch.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Video Speed"
  }
}